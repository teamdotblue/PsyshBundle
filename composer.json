{
    "name": "teamdotblue/psysh-bundle",
    "description": "A port of the PHP REPL PsySH for Symfony.",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "console",
        "psysh",
        "shell",
        "REPL",
        "symfony",
        "interactive"
    ],
    "require": {
        "php": ">=8.2",
        "psy/psysh": "^0.11 || ^0.12",
        "symfony/error-handler": "^6.4 || ^7.0",
        "symfony/expression-language": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.8",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TeamDotBlue\\PsyshBundle\\": "src"
        },
        "files": [
            "src/psysh.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TeamDotBlue\\PsyshBundle\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "symfony/*": true
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
